{
    "log": {
        "level": "info"
    },
    "dns": {
        "servers": [
            {
                "tag": "alidns",
                "address": "https://223.5.5.5/dns-query",
                "strategy": "ipv4_only",
                "detour": "direct"
            },
            {
                "tag": "cfdns",
                "address": "https://1.1.1.1/dns-query",
                "strategy": "ipv4_only",
                "detour": "AperNet"
            },
            {
                "tag": "dns_block",
                "address": "rcode://success"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "alidns"
            },
            {
                "domain_suffix": ".cn",
                "server": "alidns"
            },
            {
                "rule_set": "geosite-cn",
                "server": "alidns"
            }
        ],
        "final": "cfdns",
        "disable_cache": false,
        "disable_expire": false
    },
    "inbounds": [
        {
            "tag": "tun-in",
            "type": "tun",
            "inet4_address": "172.16.0.1/30",
            "inet6_address": "fd00::1/126",
            "mtu": 9000,
            "auto_route": true,
            "strict_route": true,
            "udp_timeout": "5m",
            "stack": "system",
            "sniff": true,
            "sniff_override_destination": true
        }
    ],
    "outbounds": [
        {
            "tag": "Select",
            "type": "selector",
            "outbounds": [
                "AperNet",
                "DMIT"
            ],
            "default": "AperNet",
            "interrupt_exist_connections": false
        },
        {
            "tag": "Telegram",
            "type": "selector",
            "outbounds": [
                "AperNet",
                "DMIT"
            ],
            "default": "AperNet",
            "interrupt_exist_connections": false
        },
        {
            "tag": "AperNet",
            "type": "shadowsocks",
            "method": "aes-256-gcm",
            "password": "ZCLQ20020312",
            "udp_over_tcp": {
                "enabled": true,
                "version": 2
            },
            "detour": "shadowtls-hk"
        },
        {
            "tag": "shadowtls-hk",
            "type": "shadowtls",
            "server": "hk.zdcy.org",
            "server_port": 8443,
            "version": 3,
            "password": "ZCLQ20020312",
            "tls": {
                "enabled": true,
                "server_name": "itunes.apple.com"
            }
        },
        {
            "tag": "DMIT",
            "type": "shadowsocks",
            "method": "aes-256-gcm",
            "password": "ZCLQ20020312",
            "udp_over_tcp": {
                "enabled": true,
                "version": 2
            },
            "detour": "shadowtls-us"
        },
        {
            "tag": "shadowtls-us",
            "type": "shadowtls",
            "server": "us.zdcy.org",
            "server_port": 8443,
            "version": 3,
            "password": "ZCLQ20020312",
            "tls": {
                "enabled": true,
                "server_name": "itunes.apple.com"
            }
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "rule_set": [
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/SagerNet/sing-geoip@rule-set/geoip-cn.srs",
                "download_detour": "AperNet"
            },
            {
                "tag": "geosite-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-cn.srs",
                "download_detour": "AperNet"
            },
            {
                "tag": "geosite-private",
                "type": "remote",
                "format": "binary",
                "url": "https://cdn.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-private.srs",
                "download_detour": "AperNet"
            },
            {
                "tag": "BiliBili",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/BiliBili.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "AI",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/AI.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "TikTok",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/TikTok.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "Twitter",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/Twitter.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "YouTube",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/YouTube.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "Telegram",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/Telegram.json",
                "download_detour": "AperNet"
            },
            {
                "tag": "E-Hentai",
                "type": "remote",
                "format": "source",
                "url": "https://cdn.jsdelivr.net/gh/Moorigin/Rule@main/Sing-Box/E-Hentai.json",
                "download_detour": "AperNet"
            }
        ],
        "rules": [
            {
                "protocol": "dns",        // 使用DNS协议的流量
                "outbound": "dns-out"     // 将通过'dns-out'出口转发
            },
            {
                "clash_mode": "direct",   // Clash模式为直连
                "outbound": "direct"      // 将通过'direct'出口直接连接
            },
            {
                "clash_mode": "global",   // Clash模式为全局
                "outbound": "Select"      // 将通过'select'出口选择转发
            },
            {
                "rule_set": "Telegram",
                "outbound": "Telegram"
            },
            {
                "rule_set": "BiliBili",
                "outbound": "AperNet"
            },
            {
                "domain_suffix": [".zdcy.org",".misakaf.org"],
                "rule_set": ["AI","TikTok","Twitter","YouTube","E-Hentai"],
                "outbound": "DMIT"
            },
            {
                "ip_is_private": true,
                "rule_set": ["geoip-cn","geosite-cn","geosite-private"],
                "outbound": "direct"
            },
            {
                "protocol": "quic",
                "port": [443],
                "outbound": "block"
            }
        ],
        "final": "Select",
        "auto_detect_interface": true
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "external_ui_download_url": "",
            "default_mode": "rule",
            "external_ui_download_detour": "Select"
        },
        "cache_file": {
            "enabled": true,
            "path": "cache.db",
            "store_fakeip": true
        }
    }
}